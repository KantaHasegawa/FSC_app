<div class="col label-margin">
  <div class="account-wall">
    <div class="chase-logo">
      <h1 class="text-center login-title top-interval">バンド情報を編集する</h1>
      <p class="text-center normal-text ">必要な情報を入力してください</p>
    </div>
  <%= react_component("BandEdit",token: form_authenticity_token, band_id: params[:id], band_name: @band.name, band_image: @band.image, members: @members ) %>
  <div class="row">
  <div class="col"><%= link_to 'キャンセル', @band, class: 'btn btn-info btn-fixed-width' %></div>
  <div class="col">  <%= link_to "バンドを削除する", @band, method: :delete, data: {confirm: "本当によろしいですか？"}, class: 'btn btn-danger btn-fixed-width' %></div>

  </div>


</div>
</div>
</div>
